{
  "name": "wild-next",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "clear": "rimraf build && rimraf dist && rimraf node_modules/.cache",
    "dev": "next",
    "build": "next build",
    "analyze": "ANALYZE=true npm run build",
    "start": "next start",
    "generate": "npm run build && next export -o dist",
    "format": "prettier --write 'src/**/*.{json,md,css,scss,less}'",
    "lint": "tsc --noEmit && eslint --color 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "npm run lint -- --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "npm run lint:fix"
    ],
    "src/**/*.{json,md,css,scss,less}": [
      "npm run format"
    ]
  },
  "dependencies": {
    "lodash": "4.17.20",
    "next": "10.0.1",
    "next-react-svg": "1.1.2",
    "next-seo": "4.15.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "styled-components": "5.2.1",
    "twin.macro": "1.12.0"
  },
  "devDependencies": {
    "@madebywild/eslint-config": "0.1.1",
    "@madebywild/prettier-config": "0.1.0",
    "@next/bundle-analyzer": "10.0.1",
    "@types/lodash": "4.14.165",
    "@types/node": "14.14.6",
    "@types/react": "16.9.56",
    "@types/styled-components": "5.1.4",
    "@types/webpack-env": "1.15.3",
    "babel-plugin-lodash": "3.3.4",
    "eslint": "7.13.0",
    "husky": "4.3.0",
    "lint-staged": "10.5.1",
    "prettier": "2.1.2",
    "rimraf": "3.0.2",
    "tailwindcss": "1.9.6",
    "typescript": "4.0.5"
  }
}

{
  "name": "wild-next",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "clear": "rimraf build && rimraf dist && rimraf node_modules/.cache",
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "generate": "npm run build && next export -o dist",
    "lint": "tsc --noEmit && eslint --color 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "npm run lint -- --fix"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not ie <= 11",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "npm run lint:fix"
    ],
    "src/**/*.json": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "lodash": "4.17.20",
    "next": "9.5.5",
    "next-react-svg": "1.1.2",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "styled-components": "5.2.0",
    "twin.macro": "1.10.0"
  },
  "devDependencies": {
    "@madebywild/eslint-config": "0.1.1",
    "@madebywild/prettier-config": "0.1.0",
    "@types/lodash": "4.14.162",
    "@types/node": "14.11.8",
    "@types/react": "16.9.52",
    "@types/styled-components": "5.1.4",
    "@types/webpack-env": "1.15.3",
    "babel-plugin-lodash": "3.3.4",
    "eslint": "7.11.0",
    "husky": "4.3.0",
    "lint-staged": "10.4.0",
    "prettier": "2.1.2",
    "rimraf": "3.0.2",
    "tailwindcss": "1.9.2",
    "typescript": "4.0.3"
  }
}
